{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "noEmit": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "lib": ["dom", "es6", "es7"],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "importHelpers": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true
  }
}
